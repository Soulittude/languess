{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "expo-sqlite": [
        "node_modules/expo-sqlite"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
